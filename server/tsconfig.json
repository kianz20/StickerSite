{
	"compilerOptions": {
		"target": "es2016",
		"jsx": "preserve",
		"module": "commonjs",
		"allowJs": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "client"],
	"files": ["types/express.d.ts"]
}
